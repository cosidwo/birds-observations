<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".activities.DashboardActivity"
    android:background="#164A41">

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/coordinator"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <com.google.android.material.textview.MaterialTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Wyloguj siÄ™"
            android:textSize="26dp"
            android:layout_marginTop="3dp"
            android:layout_marginRight="5dp"
            android:textColor="@color/white"
            android:layout_gravity="right"
            android:id="@+id/logoutTextView">
        </com.google.android.material.textview.MaterialTextView>
        <com.google.android.material.textview.MaterialTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12dp"
            android:id="@+id/mapKey"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="625dp">
        </com.google.android.material.textview.MaterialTextView>
        <fragment
            android:layout_width="match_parent"
            android:layout_height="550dp"
            android:layout_marginTop="65dp"
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment">

        </fragment>

        <com.google.android.material.bottomappbar.BottomAppBar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/bottomApp"
            android:layout_gravity="bottom"
            android:backgroundTint="#4D774E"
            >
        </com.google.android.material.bottomappbar.BottomAppBar>
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_baseline_add_24"
            app:layout_anchor="@id/bottomApp"
            android:backgroundTint="#F1B24A">
        </com.google.android.material.floatingactionbutton.FloatingActionButton>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/own_list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginLeft="20dp"
            android:backgroundTint="#4D774E"
            app:borderWidth="0dp"
            android:outlineSpotShadowColor="@color/statusColor"
            app:tint="#F1B24A"
            app:srcCompat="@drawable/ic_baseline_featured_play_list_24">
        </com.google.android.material.floatingactionbutton.FloatingActionButton>
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginLeft="100dp"
            android:backgroundTint="#4D774E"
            app:borderWidth="0dp"
            android:outlineSpotShadowColor="@color/statusColor"
            app:tint="#F1B24A"
            app:srcCompat="@drawable/ic_baseline_search_24">
        </com.google.android.material.floatingactionbutton.FloatingActionButton>
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/gallery"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_marginRight="20dp"
            android:backgroundTint="#4D774E"
            app:borderWidth="0dp"
            app:tint="#F1B24A"
            android:outlineSpotShadowColor="@color/statusColor"
            app:srcCompat="@drawable/ic_baseline_photo_library_24">
        </com.google.android.material.floatingactionbutton.FloatingActionButton>
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:outlineSpotShadowColor="@color/statusColor"
            android:layout_gravity="bottom|end"
            android:layout_marginRight="100dp"
            android:backgroundTint="#4D774E"
            app:borderWidth="0dp"
            app:tint="#F1B24A"
            app:srcCompat="@drawable/ic_baseline_person_20">
        </com.google.android.material.floatingactionbutton.FloatingActionButton>

    </androidx.coordinatorlayout.widget.CoordinatorLayout>

</androidx.constraintlayout.widget.ConstraintLayout>